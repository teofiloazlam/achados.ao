generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model documento {
  id          Int      @id @default(autoincrement())
  nome        String
  titular     String
  createAt    DateTime @default(now())
  atualizadoE DateTime @updatedAt
}
